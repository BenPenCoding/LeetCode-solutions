class Solution:
    def isValid(self, s: str) -> bool:
        
        typeDict = {"[":"]",
                    "(":")",
                    "{":"}"}

        stack = []

        for letter in s:

            if letter in ["(","[", "{"]:

                stack.append(letter)
            
            else:

                if stack and letter == typeDict[stack[len(stack) - 1]]:
                    
                    stack.pop()
                
                else:

                    return False
        
        if stack:

            return False
            
        else:
            
            return True
